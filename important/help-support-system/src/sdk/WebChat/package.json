  "dependencies": {
    "@microsoft/signalr": "^7.0.2",
    "dayjs": "^1.11.7",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.2",
    "@parcel/transformer-inline-string": "^2.8.2",
    "@parcel/transformer-typescript-types": "^2.8.2",
    "@types/node": "^18.11.18",
    "json-server": "^0.17.1",
    "parcel": "^2.8.2",
    "process": "^0.11.10",
    "rimraf": "^4.0.7",
    "typescript": "^4.9.4"
  },
  "targets": {
    "main": false,
    "module": false,
    "modern": {
      "engines": {
        "browsers": "last 2 versions"
      }
    },
    "legacy": {
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "engines": {
    "node": ">= 16"
  }
